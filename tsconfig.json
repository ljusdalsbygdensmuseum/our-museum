{
	"compilerOptions": {
		"outDir": "./build/",
		"jsx": "react-jsx",
		"target": "ES6",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"allowJs": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": "./",
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": ["src", "src/**/*.ts", "src/**/*.tsx", "src/**/block.json"],
	"exclude": ["assets/js", "build", "node_modules", "vendor"]
}
